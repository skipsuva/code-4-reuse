<p id="notice"><%= notice %></p>

<h1>Listing Shoes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Size</th>
      <th>Color</th>
      <th>Img url</th>
      <th>Src url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @shoes.each do |shoe| %>
      <%= render '/shoes/shoe', shoe: shoe %>
    <% end %>

  </tbody>
</table>

<br>

<%= link_to 'New Shoe', new_shoe_path %>

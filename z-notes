rails g scaffold Store name:string
rails g scaffold Shoes name:string style:string size:integer color:string img_url:string src_url:string
rails g scaffold Shirts name:string style:string size:integer neck:integer img_url:string src_url:string
rails g scaffold Pants name:string style:string inseam:integer waist:integer img_url:string src_url:string
